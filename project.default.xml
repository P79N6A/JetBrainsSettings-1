<application>
  <component name="ProjectManager">
    <defaultProject>
      <component name="MessDetector">
        <phpmd_settings>
          <MessDetectorConfiguration tool_path="$USER_HOME$/AppData/Roaming/Composer/vendor/bin/phpmd" />
        </phpmd_settings>
      </component>
      <component name="PhpCodeSniffer">
        <phpcs_settings>
          <PhpCSConfiguration max_messages_per_file="100" standards="MySource;PEAR;PSR1;PSR2;Squiz;Zend" tool_path="$USER_HOME$/AppData/Roaming/Composer/vendor/bin/phpcs" timeout="30000" />
        </phpcs_settings>
      </component>
      <component name="PhpWorkspaceProjectConfiguration" backward_compatibility_performed="true" />
      <component name="PropertiesComponent">
        <property name="nodejs_interpreter_path.stuck_in_default_project" value="undefined stuck path" />
        <property name="nodejs_npm_path_reset_for_default_project" value="true" />
        <property name="settings.editor.selected.configurable" value="configurable.group.appearance" />
      </component>
    </defaultProject>
  </component>
</application>